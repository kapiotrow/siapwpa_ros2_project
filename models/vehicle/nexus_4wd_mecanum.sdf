              <resolution>1</resolution>
              <min_angle>-3.14159</min_angle>
              <max_angle>3.14159</max_angle>
            </horizontal>
            <vertical>
              <samples>16</samples>         <!-- np. dla VLP‑16 -->
              <resolution>1</resolution>
              <min_angle>-0.261799</min_angle>   <!-- ~ –15° -->
              <max_angle>0.261799</max_angle>    <!-- ~ +15° -->
