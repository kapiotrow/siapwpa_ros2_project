          </scan>
          <range>
            <min>0.9</min>      <!-- domyślne z velodyne_simulator -->
            <max>130.0</max>
            <resolution>0.01</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.008</stddev>    <!-- domyślna wartość z pluginu -->
